cat << EOF > evolution.json
{
  "type": "record",
  "name": "evolution",
  "doc": "Nowa schema",
  "namespace": "pl.aggregator",
  "fields": [
    {
      "name": "name",
      "type": "string"
    },
    {
      "name": "number1",
      "type": "int"
    },
    {
      "name": "number2",
      "type": "float"
    }
  ]
}
EOF
curl -X POST -H "Content-Type: application/json" -H "Accept: application/json" -d @evolution.json http://localhost:3030/api/proxy-sr/subjects/test-schema-artur2/versions